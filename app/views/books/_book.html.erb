<div class="col-md-3">
  <div class="thumbnail">
    <%= link_to book_path(book) do  %>
    <%= image_tag "rails-book.jpeg", class: "img-responsive" %>
    <% end %>
    <div class="caption">
      <p><strong><%= link_to book.name, book_path(book) %></strong></p>
      <p><%= book.price %></p>
    </div>
  </div>
</div>
